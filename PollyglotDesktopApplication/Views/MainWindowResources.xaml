    </DataTemplate>

    <DataTemplate DataType="{x:Type vmRaport:RaportPlatnosciViewModel}">
        <viewsRaport:RaportPlatnosciView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmRaport:RaportFrekwencjiViewModel}">
        <viewsRaport:RaportFrekwencjiView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmRaport:RaportWynagrodzenViewModel}">
        <viewsRaport:RaportWynagrodzenView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmAdd:NewUczenViewModel}">
        <viewsAdd:NewUczenView />
